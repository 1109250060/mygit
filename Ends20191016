
package test;


public class Test {

    
    public static void main(String[] args) {
        int j;
        for(int i = 1;i <= 1000;i++){
            int sum = 0;//注意初始sum为零
            for(j = 1;j <= i/2;j++){
                if(i % j == 0)
                    sum = sum + j;
            }
            if(i == sum)
                System.out.println(i);
        }
    }
    
}
